@model IEnumerable<SmartGuardPortalv1.Models.ContactSchedule>

@{
    ViewBag.Title = "Index";
    int userId = (int)WebSecurity.CurrentUserId;
}

<b> @Html.ActionLink("Dashboard", "Contact", "Home") > My Contact Schedules </b>
<br />
<br />
@Html.ActionLink("Create a new schedule", "Create")

<table>


@foreach (var item in Model.Where(i => i.fkUserId == userId)) {
    <tr>
        <td>
            @item.ContactSchedules
                
        </td>
        
        
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ContactScheduleId }) 
            @* Html.ActionLink("Details", "Details", new { id=item.ContactId }) | *@
            
            
            
        </td>
    </tr>
}

</table>
