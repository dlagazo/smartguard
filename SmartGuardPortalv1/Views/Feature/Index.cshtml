@model IEnumerable<SmartGuardPortalv1.Models.Feature>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FeatureName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FeatureFxns)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FeatureName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FeatureFxns)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FeatureId }) |
            @* Html.ActionLink("Details", "Details", new { id=item.FeatureId }) | *@
            @Html.ActionLink("Delete", "Delete", new { id=item.FeatureId })
        </td>
    </tr>
}

</table>
