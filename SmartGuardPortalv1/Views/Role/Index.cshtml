@{
    ViewBag.Title = "Index";
    
}

<b>@Html.ActionLink("Dashboard", "Administrator", "Home") > Users and Roles</b>
<br />
@Html.ActionLink("Create New", "AdminAccount", "Account")
@{
    foreach(string role in Roles.GetAllRoles())
    {
        <b>@role</b> 
        <br /> 
        
       
        foreach(string user in Roles.GetUsersInRole(role))
        {
            @user
            <br />
        }
        <br />
        
    }
}

    



